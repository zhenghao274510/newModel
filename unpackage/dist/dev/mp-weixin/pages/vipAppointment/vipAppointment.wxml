<view class="contain"><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" Color="{{Color}}" backColor="{{backColor}}" isIcon="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">填写订单</view></cu-custom></view><view class="formData"><view class="list"><view>贵宾厅</view><view class="right">{{name}}</view></view><view class="list"><view>到厅时间</view><view data-event-opts="{{[['tap',[['choiceTime',['$event']]]]]}}" class="right" style="color:#999;" bindtap="__e">{{time}}</view></view><view class="list"><view>到厅人数</view><view class="right" style="background:none;padding-left:0;"><van-stepper bind:input="__e" vue-id="2" integer min="{{1}}" disable-input input-width="32px" value="{{value}}" data-event-opts="{{[['^input',[['__set_model',['','value','$event',[]]]]]]}}" bind:__l="__l"></van-stepper></view></view><view class="list"><view>联系人</view><view class="right"><input type="text" placeholder="请输入联系人姓名" data-event-opts="{{[['input',[['__set_model',['','user','$event',[]]]]]]}}" value="{{user}}" bindinput="__e"/></view></view><view class="list"><view>手机号</view><view class="right"><input type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view></view></view><view class="notice"><view class="title" style="margin-bottom:30rpx;">服务须知</view><view class="mark">{{notice}}</view></view><block wx:if="{{isVip==0}}"><view class="pay"><view class="money">实付款：<text>{{"￥"+price}}</text></view><view data-event-opts="{{[['tap',[['paySuc',['$event']]]]]}}" class="payBtn" bindtap="__e">立即支付</view></view></block><block wx:if="{{isVip==1}}"><view data-event-opts="{{[['tap',[['goOrder',['$event']]]]]}}" class="sort" bindtap="__e">立即预约</view></block><van-popup bind:input="__e" vue-id="3" position="bottom" overlay="{{false}}" value="{{show}}" data-event-opts="{{[['^input',[['__set_model',['','show','$event',[]]]]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><van-datetime-picker vue-id="{{('4')+','+('3')}}" type="datetime" min-date="{{minDate}}" value="{{currentTime}}" data-event-opts="{{[['^cancel',[['cancel']]],['^confirm',[['confirm']]],['^input',[['__set_model',['','currentTime','$event',[]]]]]]}}" bind:cancel="__e" bind:confirm="__e" bind:input="__e" bind:__l="__l"></van-datetime-picker></van-popup></view>