<view class="contain"><view class="top"><view class="title"><text data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="cuIcon-back back" bindtap="__e"></text><view class="current">{{''+date1+" "+day1+''}}</view></view></view><view class="detail"><view class="wrapper"><view class="choiceTitle"><view>{{choiceDate}}</view><view style="margin-left:20rpx;">{{city1+"-"+city2}}</view></view><view class="clock"><view class="start"><view class="time">{{list.DepartTime}}</view><view>{{($root.g0>-1?$root.g1:list.DepartAirportName)+list.DepartAirportTerminal}}</view></view><view class="start"><view style="font-size:12px;">{{list.Duration}}</view><image src="{{$root.m0}}" alt class="_img"></image></view><view class="start"><view class="time">{{list.ArriveTime}}</view><view>{{($root.g2>-1?$root.g3:list.ArriveAirportName)+list.ArriveAirportTerminal}}</view></view></view><view class="bottomList">{{list.AirCompanyName+list.FlightNo+" | "+list.PlaneType}}</view></view></view><view class="result"><scroll-view class="airs" style="height:100%;" scroll-y scroll-with-animation><block wx:for="{{list.Cabins}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="airs-list"><view class="airs-top"><view class="airs-price"><view style="font-size:20px;color:#E02B11;">{{"￥"+v.Fare}}</view><block wx:if="{{v.BabyFare!=0}}"><view style="font-size:12px;color:#999;">婴儿<text style="color:#E02B11;">{{"￥"+v.BabyFare}}</text></view></block></view><view data-event-opts="{{[['tap',[['goOrder',[k]]]]]}}" class="air-btns" bindtap="__e">预定</view></view><view class="airs-mark"><view>极速出票</view><view>实时政策</view></view><view class="recommend">{{v.CabinName+v.Discount+"折"}}</view></view></block></scroll-view></view></view>