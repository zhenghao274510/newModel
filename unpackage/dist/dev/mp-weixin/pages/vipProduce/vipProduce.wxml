<view class="contain"><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" Color="{{Color}}" backColor="{{backColor}}" isIcon="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">贵宾厅详情</view></cu-custom></view><swiper class="screen-swiper round-dot" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="5000" duration="500"><block wx:for="{{imgs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item}}" mode="aspectFill"></image></swiper-item></block></swiper><view class="air"><view class="air-text"><view>{{listData.title}}</view><text>{{"营业时间："+listData.openTime}}</text></view><image src="{{$root.m0}}" alt data-event-opts="{{[['tap',[['goService',['$event']]]]]}}" bindtap="__e" class="_img"></image></view><view class="location"><view class="title">位置说明</view><view class="produce">{{listData.address}}</view></view><view class="service"><view class="title">服务内容</view></view><view class="classify_list"><block wx:for="{{serviceList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['goContent',['$0','$1'],[[['serviceList','',k,'name']],[['serviceList','',k,'content']]]]]]]}}" class="list_type" bindtap="__e"><image mode="scaleToFill" src="{{v.icon}}"></image><text>{{v.name}}</text></view></block></view><view class="notice"><view class="title" style="margin-bottom:20rpx;">服务须知</view><view class="mark">{{listData.introduction}}</view></view><view data-event-opts="{{[['tap',[['goOrder',['$event']]]]]}}" class="sort" bindtap="__e">立即预约</view></view>