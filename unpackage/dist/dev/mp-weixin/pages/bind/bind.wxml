<view class="contain"><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" Color="{{Color}}" backColor="{{backColor}}" isIcon="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content','right']}}"><view slot="backText"></view><view slot="content">绑定会员卡</view><view slot="right"></view></cu-custom></view><view class="produce"><view class="_p">请填写线下会员实体卡信息，一经绑定不可取消</view></view><view class="message"><view class="user_msg">实体卡信息</view><view class="cu-form-group"><view class="title">姓名</view><input placeholder="请输入您的姓名" name="input" data-event-opts="{{[['input',[['__set_model',['','user','$event',[]]]]]]}}" value="{{user}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机</view><input placeholder="请输入开卡手机" name="input" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">会员卡号</view><input placeholder="请输入会员卡号" name="input" data-event-opts="{{[['input',[['__set_model',['','card','$event',[]]]]]]}}" value="{{card}}" bindinput="__e"/></view></view><view class="bind_card"><view data-event-opts="{{[['tap',[['bindCard',['$event']]]]]}}" class="card_btn" bindtap="__e">绑定会员卡</view></view><model vue-id="2" show="{{show}}" title="邦卡成功" content="可在线上享受会员卡同等特权" show-cancel="{{false}}" data-event-opts="{{[['^confirm',[['confirm']]]]}}" bind:confirm="__e" bind:__l="__l"></model></view>