<view class="contain"><view class="fixed"><cu-custom vue-id="1" isBack="{{true}}" Color="{{Color}}" backColor="{{backColor}}" isIcon="{{true}}" bgColor="bg-shadeTop text-white" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view slot="content">会员卡订单</view></cu-custom></view><view class="wrapper"><view class="tabs"><block wx:for="{{datas}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['change',[k]]]]]}}" class="{{['tab',itemIndex==k?'active':'']}}" bindtap="__e">{{v}}</view></block></view></view><mescroll-uni vue-id="2" down="{{downOption}}" up="{{upOption}}" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]],['^init',[['mescrollInit']]],['^emptyclick',[['emptyClick']]],['^topclick',[['topClick']]]]}}" bind:down="__e" bind:up="__e" bind:init="__e" bind:emptyclick="__e" bind:topclick="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="lists"><block wx:for="{{list}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['list','',k,'orderId']]]]]]]}}" class="list" bindtap="__e"><view class="list-top"><view class="name">{{"订单号："+v.orderId}}</view><block wx:if="{{v.state==0}}"><view class="state">待付款</view></block><block wx:if="{{v.state==1}}"><view class="state">交易完成</view></block></view><view class="list-all"><view class="list-location"><view>{{''+(v.type==1?'黄金':'钻石')+'会员'}}<block wx:if="{{v.category==1}}"><text>开卡</text></block><block wx:if="{{v.category==2}}"><text>续费</text></block><block wx:if="{{v.category==3}}"><text>升级</text></block></view><view class="state" style="font-size:32rpx;">{{"￥"+v.amount+"元"}}</view></view><view class="date">{{v.createDate}}</view></view></view></block></view></mescroll-uni></view>